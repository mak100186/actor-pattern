{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ActorFrameworkOptions": {
    "MailboxType": "Bounded",
    "MailboxCapacity": 500,
    "MailboxOverflowPolicy": "BlockProducer",
    "ShouldStopOnUnhandledException": true,
    "RetryCountIfExceptionOccurs": 3
  }
}
